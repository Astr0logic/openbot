{
    "name": "secure-ai-sdk",
    "version": "0.1.0",
    "description": "Zero-latency, drop-in SDK for generating immutable audit receipts for AI API calls",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "test": "vitest run",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "ai",
        "audit",
        "receipts",
        "security",
        "openai"
    ],
    "license": "MIT",
    "dependencies": {
        "openai": "^4.104.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@types/uuid": "^9.0.0",
        "typescript": "^5.0.0",
        "vitest": "^2.0.0"
    },
    "peerDependencies": {
        "openai": ">=4.0.0"
    }
}