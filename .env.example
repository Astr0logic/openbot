# Copy to .env and fill with your Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# OpenClaw Gateway Security
OPENCLAW_GATEWAY_TOKEN=your_secure_token_here
# Required for stable encryption across container restarts
OPENCLAW_ENCRYPTION_KEY=generate_with_openssl_rand_hex_32

# Claude AI Session (Optional)
CLAUDE_AI_SESSION_KEY=sk-ant-sd...
CLAUDE_WEB_SESSION_KEY=sessionKey=...
CLAUDE_WEB_COOKIE=...
